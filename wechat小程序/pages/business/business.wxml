<!--pages/business/business.wxml-->
<view class="weui-panel weui-panel_access">
            <view class="weui-panel__hd">展商列表</view>
            <view class="weui-panel__bd">
                <view  wx:for="{{business}}" wx:for-index="idx" wx:for-item="itemName"class="weui-media-box weui-media-box_text">
                    <view class="weui-media-box__title weui-media-box__title_in-text">{{itemName.businessName}}</view>
                    <view class="weui-media-box__desc">{{itemName.businessBooth}}</view>
                </view>
            </view>
        </view>
